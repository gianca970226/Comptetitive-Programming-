def divide(a,i,j):
    if (j-1)==1:
        if(a[i])
def main():
    while(True):
        n=int(input())
        if not n:
            break;
        else:
            for j in range(0,n):
                res=0;
                x,y=input().split()
                v=int(x)
                b=int(y)
                a=input().split(" ")
                aux=[0]*pow(2,int(v))
                for i in range(0,int(b)):
                    aux[int(a[i])-1]=1
                 
                print (res)
            break   
                    
if __name__ == '__main__':
    main()