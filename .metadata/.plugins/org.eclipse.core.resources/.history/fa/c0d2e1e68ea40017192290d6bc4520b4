class date:
    def __init__(self,numTeam,solved,time):
        self.numTeam=numTeam
        self.solved=solved
        self.time=time
def findDuration(time):
    x,y=time.split(":")
    return (int(x))*60+int(y)
def main():
    n= int(input())
    input()
    for i in range(0,n):
        line=input()
        while line!="": 
            datos=input.split("")
            status=datos[3]
            if status=="Y":
                time=findDuration(datos[2])
            line=input()
            
                
    
if __name__ == '__main__':
    main()