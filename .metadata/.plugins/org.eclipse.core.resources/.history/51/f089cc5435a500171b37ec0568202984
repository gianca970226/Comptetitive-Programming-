x=0
def divide(a,i,j):
    if (j-1)==1:
        if(a[i]==1 and a[j]==1):
            return 0
        elif((a[i]==1 and a[j]==0)or (a[i]==0 and a[j]==1))
            return 1
            a=a+1
        else:
            return 1
def main():
    while(True):
        n=int(input())
        if not n:
            break;
        else:
            for j in range(0,n):
                res=0;
                x,y=input().split()
                v=int(x)
                b=int(y)
                a=input().split(" ")
                aux=[0]*pow(2,int(v))
                for i in range(0,int(b)):
                    aux[int(a[i])-1]=1
                 
                print (res)
            break   
                    
if __name__ == '__main__':
    main()