
import re, string

def replace_punctuation ( text, replace ):
    return re.sub('[%s]' % re.escape(string.punctuation), replace, text)
  
def main():
    n=int(input())
    for j in range(0,n):
        line=input()
        while line!="END":
            line=replace_punctuation(line, " ")
            line=
            line = " ".join( line.split() )
            print(line.strip())
            line=input()
    
    
if __name__=="__main__":
    main()