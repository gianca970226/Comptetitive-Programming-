import sys
def main():
    
    while(True):
        n=int(input())
        if n==0:
            break;
        else:
            grid =[[0]*n+1]*3
            for j in range(0,n):
                x,y=input().split()
                grid[0][x]=x
                grid[1][x]=y
            
                print(grid)
    

if __name__ == '__main__':
    main()